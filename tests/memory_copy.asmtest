import memory;

u64 test_u64()
{
    u64 a = 137;
    u64 b = 0;

    memory::copy(&a, &b, sizeof(u64));

    return b;
}

u64 test_cstring()
{
    u8* a = "hi";
    u8[3] b;

    memory::copy(a, &b, 3);

    return b[0] == 'h' && b[1] == 'i' && b[2] == 0;
}

u64 main()
{
    return test_{{ test }}();
}

